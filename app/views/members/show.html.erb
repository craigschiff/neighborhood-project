
<h1> This is the member show page </h1>
<div>
<p>Name: <%=current_member.name.capitalize%></p>
<p>About Me: <%=current_member.about%></p>
<p>Gender: <%=current_member.gender.capitalize%></p>
<p>Age: <%=current_member.age%></p>
<p>Relationship Status: <%=current_member.relationship_status.capitalize%></p>
<p>Hometown: <%=current_member.hometown%></p>
<p>Phone Number: <%=current_member.phone%></p>
</div>
<div>
<%current_member.rsvps.each do |rsvp|%>
<%end%>
</div>

<%= month_calendar events: current_member.rsvps do |date, rsvps| %>
  <%= date.strftime("%m/%d") %>

  <% rsvps.each do |rsvp| %>
    <div>
      <%= link_to rsvp.event.name, rsvp.event %>
    </div>
  <% end %>
<% end %>
<div>
<%=link_to 'Edit Your Profile', edit_member_path(current_member)%>
</div>
